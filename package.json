{
  "name": "zadanie_26_4",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:html",
    "test:html": "globstar nu-html-checker index.html",
    "init-project": "npm install && npm-run-all init:*",
    "init:mkdirp": "npm install mkdirp",
    "init:run-all": "npm install npm-run-all --save-dev",
    "init:dirs": "mkdirp sass css vendor images js",
    "init:files": "touch README.md index.html sass/style.scss js/script.js",
    "init:gitignore": "curl https://raw.githubusercontent.com/github/gitignore/master/Node.gitignore -o .gitignore",
    "build": "npm-run-all build:*",
    "build:clean": "rimraf css/*.*",
    "build:sass": "node-sass --output-style compact -o css sass",
    "build:autoprefixer": "autoprefixer-cli css/style.css",
    "build-dev:sass": "node-sass --output-style expanded --source-map true -o css sass",
    "watch": "npm-run-all build -p watch:*",
    "watch:sass": "node-sass --output-style expanded --source-map true -o css sass --watch",
    "watch:autoprefixer": "onchange -d 100 css/style.css -- autoprefixer-cli -o css/style.prefixed.css css/style.css",
    "watch:browsersync": "browser-sync start --server --files css/*.css *html",
    "watch:sassprefixer": "onchange sass/*.scss -- npm-run-all build:sass build:autoprefixer"
  },
  "keywords": [],
  "author": "Maciek",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "browser-sync": "^2.26.5",
    "experss": "0.0.1-security",
    "express": "^4.17.1",
    "mkdirp": "^0.5.1",
    "passport": "^0.4.0",
    "passport-google-oauth": "^2.0.0",
    "pug": "^2.0.4",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer-cli": "^1.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^3.2.0",
    "globstar": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "nu-html-checker": "^0.1.0",
    "onchange": "^5.2.0",
    "optimize-js-plugin": "0.0.4",
    "react-hot-loader": "^4.12.13",
    "style-loader": "^1.0.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1"
  },
  "description": "Using Socket.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaciekSznu/Zadanie26.4.git"
  },
  "bugs": {
